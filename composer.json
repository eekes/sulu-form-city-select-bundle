{
    "name": "eekes/sulu-form-city-select-bundle",
    "type": "symfony-bundle",
    "description": "Adds a city select field to Sulu forms for use on the website, enabling users to easily select cities from a list within forms on the frontend.",
    "keywords": [
        "symfony",
        "sulu",
        "form",
        "city"
    ],
    "homepage": "https://www.ewaldvanderveken.dev/",
    "license": "MIT",
    "authors": [
        {
            "name": "Ewald Vanderveken",
            "email": "vandervekenewald@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "sulu/form-bundle": "^2.5",
        "sulu/sulu": "^2.6",
        "symfony/cache": "^7.0",
        "symfony/intl": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpro/grumphp": "^2.4",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Eekes\\Sulu\\FormCitySelectBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpro/grumphp": true,
            "php-http/discovery": true
        }
    }
}
